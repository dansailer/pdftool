{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-set-title",
    "core:window:allow-close",
    "opener:default",
    "dialog:default",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "i18n:default",
    "log:default",
    "updater:default",
    "process:allow-restart",
    "core:webview:allow-internal-toggle-devtools",
    "os:default"
  ]
}
